{
  "name": "gatsby-source-spotify",
  "version": "1.3.0",
  "description": "Gatsby source plugin for using data from Spotify on your website",
  "main": "index.js",
  "bin": "token-tool.js",
  "author": "Leo Bernard <admin@leolabs.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leolabs/gatsby-source-spotify.git"
  },
  "homepage": "https://github.com/leolabs/gatsby-source-spotify",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "spotify"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "format": "prettier --write 'src/**/*",
    "lint": "tslint --project tsconfig.json"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/open": "^6.1.0",
    "commander": "^2.20.0",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-source-filesystem": "^2.1.2",
    "inquirer": "^6.4.1",
    "node-fetch": "^2.6.0",
    "open": "^6.4.0"
  },
  "devDependencies": {
    "@types/inquirer": "^6.0.3",
    "@types/node": "^12.0.12",
    "@types/node-fetch": "^2.3.7",
    "@types/prettier": "^1.16.4",
    "gatsby": "^2.10.8",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "gatsby": "^2.10.8"
  }
}
